%h1= @signup.current_signup_step.text

- console
.progress
  .progress-bar{ role: :progressbar, style: "width: #{@signup.percentage}%", "aria-valuenow": "#{@signup.percentage}", "aria-valuemin": "0", "aria-valuemax": "100" }
    Step #{@signup.current_signup_step.step_number}

= render "parents/#{@signup.current_signup_step.to_s}_signups/form"
